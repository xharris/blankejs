{
  "name": "BlankE",
  "description": "BlankE game dev",
  "version": "0.1.0",
  "main": "entry.js",
  "single-instance": false,
  "chromium-args": "--enable-webgl --ignore-gpu-blacklist --allow-file-access-from-files",
  "scripts": {
    "electron": "electron .",
    "build": "electron-builder",
    "dist": "node package.js"
  },
  "build": {
    "appId": "com.electron.blanke",
    "win": {
      "target": "portable",
      "icon": "logo.ico"
    },
    "directories": {
      "buildResources": "src"
    }
  },
  "dependencies": {
    "adm-zip": "^0.4.13",
    "archiver": "^2.1.1",
    "fs-extra": "^5.0.0",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.15",
    "lodash.clonedeep": "^4.5.0",
    "markdown-it": "^9.1.0",
    "minimist": "^1.2.5",
    "node-watch": "^0.6.0",
    "request": "^2.88.0",
    "uglify-es": "^3.3.9",
    "walk": "^2.3.13"
  },
  "devDependencies": {
    "electron-packager": "^14.0.1",
    "electron": "^5.0.3",
    "electron-builder": "^22.4.1"
  }
}
